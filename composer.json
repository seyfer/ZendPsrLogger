  "homepage": "http://seyferseed.ru",
  "license": "GPL v2",
  "authors": [
    {
      "name": "seyfer",
      "email": "seyferseed@mail.ru",
      "homepage": "http://seyferseed.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5",
    "zendframework/zendframework": "~2.5",
    "doctrine/common": ">=2.4",
    "doctrine/orm": ">=2.4",
    "doctrine/doctrine-module": ">=0.8",
    "doctrine/doctrine-orm-module": ">=0.8",
    "psr/log": "1.0.*@dev"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.zendframework.com/"
    }
